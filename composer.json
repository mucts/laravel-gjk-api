            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuCTS\\Laravel\\GuiJK\\Providers\\ServiceProvider"
            ]
        },
        "aliases": {
            "Gjk": "MuCTS\\Laravel\\GuiJK\\Facades\\Gjk"
        }
    },
    "authors": [
        {
            "name": "herry",
            "email": "yuandeng@aliyun.com"

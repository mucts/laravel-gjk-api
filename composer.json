{
    "name": "mucts/laravel-gjk-api",
    "description": "GuiJK API Request",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "laravel/framework": "^7.0",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
